{
  "name": "Alarm Clock",
  "version": "1.3",
  "description": "A clock with two alarms that will play a sound and speak a phrase of your choice.",
  "permissions": [ "background", "tts" ],

  "background": { "scripts": ["common.js", "background.js"] },

  "browser_action": {
    "default_icon": "/assets/images/clock.png",
    "default_title": "Alarm Clock",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
